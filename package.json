    "grunt-contrib-uglify": "~3.4.0",
    "grunt-contrib-watch": "1.1.0",
    "grunt-gitinfo": "~0.1.9",
    "jshint-stylish": "~2.2.1",
    "load-grunt-tasks": "4.0.0",
    "material-design-icons-iconfont": "6.1.1"
  },
  "overrides": {
    "async": "~2.6.4",
